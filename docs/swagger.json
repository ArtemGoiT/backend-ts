          "404": {
            "$ref": "#/components/responses/404-2"
          },
          "500": {
            "$ref": "#/components/responses/500-2"
          }
        }
      }
    }
  },
  "components": {
    "securitySchemes": {
      "bearerAuth": {
        "type": "http",
        "scheme": "bearer"
      }
    },
    "schemas": {
      "registerUser": {
        "type": "object",
        "required": [
          "_id",
          "email"
        ],
        "properties": {
          "_id": {
            "description": "User's id",
            "type": "string",
            "example": "66e69d630a8a440495bed3cc"
          },
          "email": {
            "description": "User's email",
            "type": "string",
            "format": "email",
            "example": "user@gmail.com"
          },
          "name": {
            "description": "User's name (optional, defaults to 'User')",
            "type": "string",
            "example": "Masha"
          },
          "gender": {
            "description": "User's gender (optional, defaults to 'woman')",
            "type": "string",
            "enum": [
              "woman",
              "man"
            ],
            "example": "woman"
          },
          "dailyNorma": {
            "description": "User's dialy water consumption in liters(optional, defaults '1.5')",
            "type": "number",
            "example": 1.5
          }
        }
      },
      "patchUser": {
        "type": "object",
        "properties": {
          "_id": {
            "description": "User's id",
            "type": "string",
            "example": "66e69d630a8a440495bed3cc"
          },
          "email": {
            "description": "User's email",
            "type": "string",
            "format": "email",
            "example": "user@gmail.com"
          },
          "name": {
            "description": "User's name (optional, defaults to 'User')",
            "type": "string",
            "example": "Masha"
